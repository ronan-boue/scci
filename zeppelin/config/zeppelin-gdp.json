{
    "version": "0.3",
    "version_date": "2024-10-22",
    "log_data_filename": "/logs/zeppelin-data.log",
    "main_thread_interval_sec": 0.1,
    "pipelines": [
        {
            "name": "cloud2device",
            "class": "cloud2device",
            "json_schema": "",
            "config": "",
            "apply_global_validation_rules": false,
            "validation_rules": {},
            "max_payload_size_bytes": 65536,
            "thread_interval_sec": 0.1,
            "source_broker": {
                "class": "iotdevice",
                "topic": "none",
                "throttle_max_message_sec": 20,
                "throttle_sleep_sec": 1.0
            },
            "destination_broker": {
                "class": "iotedge",
                "topic": ""
            },
            "cloud_event": {
            }
        },
        {
            "name": "GDP",
            "class": "gdp",
            "json_schema": "/config/schemas/gdp-schema.json",
            "config": "",
            "apply_global_validation_rules": false,
            "validation_rules": {},
            "max_payload_size_bytes": 1024,
            "thread_interval_sec": 0.1,
            "source_broker": {
                "class": "iotedge",
                "topic": "gdp",
                "throttle_max_message_sec": 20,
                "throttle_sleep_sec": 1.0
            },
            "destination_broker": {
                "class": "mqtt",
                "topic": "GDP",
                "mqtt": {
                    "host": "eclipse-mosquitto",
                    "host_dev": "host.docker.internal",
                    "port": 1883,
                    "id": "zeppelin_gdp_dst",
                    "keepalive": 3600,
                    "qos": 1
                }
            }
        }
    ],
    "global_validation_rules": {
        "units": [
            "kwh",
            "w",
            "v",
            "mv",
            "a",
            "vh",
            "kw",
            "hz",
            "va",
            "hpa",
            "μs",
            "s",
            "sec",
            "min",
            "hour",
            "day",
            "month",
            "year",
            "°c",
            "%",
            "lqi",
            "lx"
        ],
        "values": [
            {
                "type": "unit",
                "name": "second",
                "units": [
                    "s"
                ],
                "min": 0,
                "max": null
            },
            {
                "type": "unit",
                "name": "micro-second",
                "units": [
                    "us",
                    "μs"
                ],
                "min": 0,
                "max": null
            },
            {
                "type": "unit",
                "name": "temperature-celsius",
                "units": [
                    "C",
                    "°C"
                ],
                "min": -100,
                "max": 100
            },
            {
                "type": "unit",
                "name": "volt",
                "units": [
                    "V"
                ],
                "min": 0,
                "max": 300
            },
            {
                "type": "unit",
                "name": "amp",
                "units": [
                    "A"
                ],
                "min": 0,
                "max": 200
            },
            {
                "type": "unit",
                "name": "cumulative-watt-hour",
                "units": [
                    "kwh"
                ],
                "min": 0,
                "max": null
            },
            {
                "type": "unit",
                "name": "watt",
                "units": [
                    "w"
                ],
                "min": 0,
                "max": 100
            }
        ]
    }
}